!-------------------------------------------------------------------------------
!
!   U R X V T
!
URxvt*geometry:     80x25
URxvt*background:   #000000
URxvt*foreground:   #ffffff
URxvt*reverseVideo: false
URxvt*tintColor:    #dfdfdf
URxvt*shading:      5 !trans=100, 0=opaque
URxvt.font:         xft:Hack:size=10
!URxvt.font:         -windows-proggyclean-medium-r-normal--13-80-96-96-c-70-iso8859-1
!URxvt.boldFont:     -windows-proggyclean-medium-r-normal--13-80-96-96-c-70-iso8859-1

URxvt*scrollBar:            false
URxvt*scrollWithBuffer:     true
URxvt*scrollBar_floating:   true
URxvt*scrollBar_right:      false
URxvt*scrollColor:          #000000
URxvt*saveLines:            9999
URxvt*termName:             xterm-256color
URxvt*cursorColor:          #d8d8d8
URxvt*colorBD:              lightyellow
URxvt*colorUL:              white
URxvt*eightBitInput:        true
URxvt*metaSendsEscape:      true
URxvt*visualBell:           false
urxvt*inheritPixmap:        true
URxvt*xftAntialias:         true
urxvt.perl-ext-common:      mark-urls
urxvt.urlLauncher:          chrome
URxvt.internalBorder:       5

!urxvt.tabbed.tabbar-fg:     0
!urxvt.tabbed.tabbar-bg:     0
!urxvt.tabbed.tab-fg:        1
!urxvt.tabbed.tab-bg:        0

! Fix home and end key on MacBook Keyboard
URxvt.keysym.Home:          \033[H
URxvt.keysym.End:           \033[F

! COLORS
!black
URxvt*color0:           #333333
URxvt*color8:           #686868
!red
URxvt*color1:           #CD8484
URxvt*color9:           #cf8187
!green
URxvt*color2:           #B5CCA0
URxvt*color10:          #A1B98B
!yellow
URxvt*color3:           #edb25d
URxvt*color11:          #dfb374
!blue
URxvt*color4:           #8CA0B0
URxvt*color12:          #7292ad
!magenta
URxvt*color5:           #BAA1C4
URxvt*color13:          #AB839D
!cyan
URxvt*color6:           #81C5C7
URxvt*color14:          #87C0C4
!white
URxvt*color7:           #CCCCCC
URxvt*color15:          #CCCCCC

